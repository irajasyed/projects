from Tkinter import *
import Tkinter as tk
import ttk
import Tkinter
root=Tkinter.Tk()
root.grid()


def buttoncmd1():
	print("")
def buttoncmd2():
	print("")
def buttoncmd3():
	print("")
def buttoncmd4():
	print("")

labels1=['Primary','Upper Primary','Secondary','Higher Secondary']

l=ttk.Button(root,text=labels1[0],command=buttoncmd0)
l.grid(column=0,row=0)

l=ttk.Button(root,text=labels1[1],command=buttoncmd1)
l.grid(column=0,row=1)
 
l=ttk.Button(root,text=labels1[2],command=buttoncmd2)
l.grid(column=0,row=2)

l=ttk.Button(root,text=labels1[3],command=buttoncmd3)
l.grid(column=0,row=3)

